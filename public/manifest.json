{
  "name": "MMW",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "BMI MMW",
  "icons": {
    "128": "mark.png"
  },
  "permissions": ["history"],
  "background": {
  "scripts": [
    "background.js"
  ],
  "persistent": true
  },
  "browser_action": {
  "default_icon": "mark.png",
  "default_popup": "popup.html"
  },
  "options_page": "index.html"
  }
